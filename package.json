{
  "name": "react-ws",
  "version": "1.0.0",
  "description": "react-ws ",
  "main": "index.js",
  "scripts": {
    "precommit": "eslint ./src/ ./demo/;",
    "pre": "npm install",
    "clear": "rm -rf build; mkdir build;",
    "start": "webpack-dev-server --config webpack.config.dev.js --inline --hot --progress --port 3000",
    "deploy": "npm install;npm run build && npm run build:min",
    "build": "NODE_ENV=production webpack",
    "build:min": "NODE_ENV=production webpack --config webpack.config.min.js",
    "publishpatch": "npm run deploy; git add --all; git commit -m 'c'; npm version patch; git push origin master:master; npm publish --registry='https://registry.npmjs.org'; cnpm sync react-gm; npm version;",
    "gh-pages": "git branch -D gh-pages; git checkout -b gh-pages; webpack --config webpack.config.dev.js; rm -rf .gitignore; git add --all; git commit -m 'page'; git push origin :gh-pages; git push origin HEAD:gh-pages; git checkout master;"
  },
  "keywords": [
    "react-ws",
    "wscreen-new"
  ],
  "author": "bonc",
  "license": "ISC",
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.23.1",
    "echarts": "^3.2.2",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gm-bootstrap": "^1.1.2",
    "gm-util": "^2.0.6",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "moment": "^2.14.1",
    "object-assign": "^4.1.0",
    "path": "^0.12.7",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "react": "^15.2.1",
    "react-bootstrap": "^0.30.0",
    "react-dom": "^15.2.1",
    "react-hot-loader": "^1.3.0",
    "react-router": "^2.6.0",
    "shallowequal": "^0.2.2",
    "style-loader": "^0.13.1",
    "underscore": "^1.8.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.12.1"
  },
  "dependencies": {
    "antd": "^1.10.0",
    "echarts": "^2.2.7-beta7",
    "node-fetch": "^1.6.0"
  }
}
